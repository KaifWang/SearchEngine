{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            // name displayed in debug pannel
            "name": "(gdb) debug",
            "type": "cppdbg",
            "request": "launch",
            // program executable, match with build result
            "program": "${fileDirname}/unit_test_network_utility.o",
            // Command Line Input
            "args": ["https://jobs.utsa.edu/"], // default
            // Stop at main()
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            //"console": "externalTerminal", // not support in c++
            "MIMode": "lldb",
            // Run build before debug
            //"preLaunchTask": "build debug"
        }
    ]
}
